services:
  api:
    build:
      context: C:\Users\andre\git\DISP_Delprojekt1\API
      dockerfile: API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: API
      target: base
    container_name: swtdisp-ap1-gr10-backend
    depends_on:
      db:
        condition: service_healthy
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: elgaard1/api:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/API.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 44376
      target: 80
    - target: 80
    - target: 443
    restart: always
    tty: true
    volumes:
    - C:\Users\andre\git\DISP_Delprojekt1\API\API:/app:rw
    - C:\Users\andre\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\andre\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\andre\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\andre\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\andre\git\DISP_Delprojekt1\API:/src:rw
  db:
    container_name: swtdisp-ap1-gr10-database
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: secretPassword1!
    healthcheck:
      test: exit 0
    image: mcr.microsoft.com/mssql/server:2019-latest
    ports:
    - published: 1433
      target: 1433
    restart: always
  gui:
    build:
      context: C:\Users\andre\git\DISP_Delprojekt1\API
      dockerfile: Frontend_Project/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Frontend_Project
    container_name: swtdisp-ap1-gr10-frontend
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: elgaard1/frontendproject:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/Frontend_Project.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 9000
      target: 80
    restart: always
    tty: true
    volumes:
    - C:\Users\andre\git\DISP_Delprojekt1\API\Frontend_Project:/app:rw
    - C:\Users\andre\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\andre\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\andre\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\andre\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\andre\git\DISP_Delprojekt1\API:/src:rw
version: '3.4'